# Hetzner Cloud API token used to create infrastructure
hcloud_token = ""

# Hetzner location used for all resources - https://docs.hetzner.cloud/#locations
hcloud_location = "fsn1"

# Image to be used for all instances - https://docs.hetzner.cloud/#images
hcloud_image = "ubuntu-22.04"

# Type of instance to be used for all instances - https://docs.hetzner.cloud/#server-types
hcloud_server = "cax11"

# Cloudflare API token used to add DNS records
cloudflare_token = ""

# Cloudflare Zone ID
cloudflare_zone_id = ""

# Domain URL to point server to
domain = "example.com"

# Prefix added to names of all resources
prefix = "example"
